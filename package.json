{
  "name": "tsdown-plugins-reexport",
  "version": "0.1.0",
  "description": "TSDown plugin for re-exporting config files",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown --exports",
    "dev": "tsdown --watch",
    "lint": "biome lint --write .",
    "format": "biome format --write .",
    "release": "release-it"
  },
  "peerDependencies": {
    "tsdown": "^0.15.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.5",
    "@types/bun": "^1.2.23",
    "@types/node": "24.6.2",
    "release-it": "^19.0.5",
    "typescript": "^5.9.3"
  },
  "module": "./dist/index.js",
  "release-it": {
    "$schema": "https://unpkg.com/release-it@19/schema/release-it.json",
    "git": {
      "commitMessage": "chore: release v${version}"
    },
    "github": {
      "release": true
    },
    "npm": {
      "publish": true
    }
  }
}